{
  "rules": [
    {
      "name": "created_by_type_inheritance",
      "description": "Type of work is inherited from its creator",
      "premises": [
        {
          "predicate": "created_by",
          "args": [
            "P1",
            "W1"
          ]
        },
        {
          "predicate": "type_of",
          "args": [
            "W1",
            "T1"
          ]
        }
      ],
      "conclusion": {
        "predicate": "type_of",
        "args": [
          "P1",
          "T1"
        ]
      }
    },
    {
      "name": "published_on_propagation",
      "description": "Publication date is propagated to the author",
      "premises": [
        {
          "predicate": "created_by",
          "args": [
            "P1",
            "W1"
          ]
        },
        {
          "predicate": "published_on",
          "args": [
            "W1",
            "D1"
          ]
        }
      ],
      "conclusion": {
        "predicate": "published_on",
        "args": [
          "P1",
          "D1"
        ]
      }
    }
  ]
}