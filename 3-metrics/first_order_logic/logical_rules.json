{
  "rules": [
    {
      "name": "Membership Inheritance",
      "description": "If an entity is a member of a group and the group has a parent, then the entity is also a member of its parent.",
      "premises": [
        {
          "predicate": "member_of",
          "args": [
            "entity",
            "group"
          ]
        },
        {
          "predicate": "has_parent",
          "args": [
            "group",
            "parent"
          ]
        }
      ],
      "conclusion": {
        "predicate": "member_of",
        "args": [
          "entity",
          "parent"
        ]
      }
    },
    {
      "name": "Creator Inheritance",
      "description": "If an entity was created by someone and the creator is a member of a group, then the entity is also a member of that group.",
      "premises": [
        {
          "predicate": "created_by",
          "args": [
            "entity",
            "creator"
          ]
        },
        {
          "predicate": "member_of",
          "args": [
            "creator",
            "group"
          ]
        }
      ],
      "conclusion": {
        "predicate": "member_of",
        "args": [
          "entity",
          "group"
        ]
      }
    },
    {
      "name": "Actor Inheritance",
      "description": "If an actor played a role in a movie and the movie was starred in by another actor, then the first actor also starred in that movie.",
      "premises": [
        {
          "predicate": "played_role",
          "args": [
            "actor",
            "role"
          ]
        },
        {
          "predicate": "starred_in",
          "args": [
            "movie",
            "actor"
          ]
        }
      ],
      "conclusion": {
        "predicate": "starred_in",
        "args": [
          "actor",
          "movie"
        ]
      }
    }
  ]
}